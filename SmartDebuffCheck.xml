<Ui xmlns="http://www.blizzard.com/wow/ui/" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<Script file="SmartDebuffCheck.lua"/>
	<Frame name="SmartDebuffCheckFrame" hidden="true">
		<Scripts>
			<OnLoad>
				SDC_OnLoad()
			</OnLoad>
			<OnEvent>
				if (event == "VARIABLES_LOADED") then SDC_OnLoad(); end
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>